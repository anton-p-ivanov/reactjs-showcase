{
  "testEnvironment": "jsdom",
  "testPathIgnorePatterns": [
    "<rootDir>/.next/",
    "<rootDir>/node_modules/",
    "<rootDir>/src/__tests__/setup.js"
  ],
  "moduleDirectories": [
    "node_modules",
    "src"
  ],
  "moduleNameMapper": {
    "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/src/__mocks__/staticFileMock.js",
    "\\.(css|scss)$": "identity-obj-proxy"
  },
  "setupFiles": [
    "<rootDir>/src/__tests__/setup.js"
  ]
}
